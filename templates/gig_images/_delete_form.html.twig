<form method="post" action="{{ path('gig_images_delete', {'id': gig_image.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ gig_image.id) }}">
    <button class="btn">Delete</button>
</form>